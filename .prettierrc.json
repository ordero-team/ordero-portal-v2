{
  "bracketSpacing": true,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 125,
  "arrowParens": "always",
  "parser": "typescript",
  "overrides": [
    {
      "files": ["*.component.html"],
      "options": {
        "parser": "angular",
        "htmlWhitespaceSensitivity": "ignore"
      }
    },
    {
      "files": ["*.ts"],
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
